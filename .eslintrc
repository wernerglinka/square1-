{
  "extends": ["plugin:@wordpress/eslint-plugin/esnext"],
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    "indent": ["error", 2],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
}
